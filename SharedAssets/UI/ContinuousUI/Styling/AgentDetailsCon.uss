:root {
    -unity-font-definition: resource('FlatSkin/Font/Roboto-Black');
    color: rgb(243, 243, 243);
}

.observation_container {
    align-items: center;
    min-width: 380px;
    width: 25%;
    min-height: 500px;
    max-width: 500px;
    max-height: 1080px;
    height: 95px;
}

.container_header {
    flex-direction: row;
    width: 100%;
    justify-content: space-between;
    align-items: center;
    background-color: rgb(43, 38, 38);
    max-height: 30px;
    padding-top: 2px;
    padding-right: 2px;
    padding-bottom: 2px;
    padding-left: 2px;
    flex-grow: 1;
    transition-duration: 0.5s;
}

.container_header:hover {
    background-color: rgb(205, 195, 195);
    color: rgb(26, 25, 25);
}

.border {
    border-top-width: 3px;
    border-right-width: 3px;
    border-bottom-width: 3px;
    border-left-width: 3px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    border-bottom-left-radius: 4px;
    border-left-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-top-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
}

.module_control {
    flex-direction: row;
    justify-content: flex-end;
    align-items: center;
    flex-grow: 1;
    align-self: auto;
}

#ControlActionIcon {
    background-image: url("project://database/Assets/UI%20Toolkit/UnityThemes/UnityDefaultRuntimeTheme.tss?fileID=4154780841294389669&guid=59783a2dcc095064d962b2c613f3c270&type=3#arrow-down");
    min-width: 20px;
    min-height: 20px;
    max-width: 30px;
    width: 20px;
    height: 20px;
    max-height: 30px;
}

#HeaderName {
    -unity-text-align: middle-center;
}

#ControlActionName {
    -unity-text-align: middle-center;
}

.horizontal_divider {
    width: 100%;
    height: auto;
    background-color: rgb(209, 209, 209);
    max-height: 3px;
    flex-grow: 1;
}

.hidden {
    visibility: visible;
    display: none;
    overflow: visible;
    width: 0;
    height: 0;
}

#ContainerContent {
    max-height: none;
    min-height: 300px;
    width: 100%;
    flex-grow: 1;
}

#AgentDetailsContentWrapper {
    background-color: rgb(238, 0, 0);
    flex-grow: 1;
}

#LifetimeLengthWrapper {
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding-top: 4px;
    padding-right: 4px;
    padding-bottom: 4px;
    padding-left: 4px;
}

#CumulativeRewardLabel {
    font-size: 16px;
    -unity-text-align: middle-center;
    -unity-font-style: normal;
}

.justify_text_center {
    -unity-text-align: middle-center;
}

.text_size_bigger {
    font-size: 16px;
}

#EnvironmentParametersWrapper {
    flex-direction: row;
    justify-content: space-between;
    align-self: center;
    padding-top: 2px;
    padding-right: 2px;
    padding-bottom: 2px;
    padding-left: 2px;
    margin-top: 2px;
    margin-right: 2px;
    margin-bottom: 2px;
    margin-left: 2px;
    align-items: center;
    width: 100%;
}

#NorthObservationLabel {
    padding-top: 2px;
    padding-right: 2px;
    padding-bottom: 2px;
    padding-left: 2px;
    -unity-text-align: middle-center;
    font-size: 14px;
}

#HorizontalWrapper {
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding-top: 4px;
    padding-right: 4px;
    padding-bottom: 4px;
    padding-left: 4px;
}

#SouthObservationLabel {
    padding-top: 2px;
    padding-right: 2px;
    padding-bottom: 2px;
    padding-left: 2px;
    -unity-text-align: middle-center;
    font-size: 14px;
}

#ActionHistoryList {
    flex-grow: 1;
    padding-top: 4px;
    padding-right: 4px;
    padding-bottom: 4px;
    padding-left: 4px;
    min-height: auto;
    height: 600px;
}

#ActionHistoryWrapper {
    flex-grow: 1;
}
